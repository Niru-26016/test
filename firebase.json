{
  "functions": {
    "source": "functions",
    "runtime": "nodejs20"
  },
  "firestore": {
    "rules": "firestore.rules"
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "idex-01",
          "appId": "1:316394099784:android:5ada23b78654b6f58eb47d",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "idex-01",
          "configurations": {
            "android": "1:316394099784:android:5ada23b78654b6f58eb47d"
          }
        }
      }
    }
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "/.well-known/assetlinks.json",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/delete-account",
        "destination": "/delete-account.html"
      },
      {
        "source": "!/.well-known/**",
        "destination": "/index.html"
      }
    ]
  }
}